<?xml version="1.0" encoding="UTF-8"?>
<Defs>
      <RecipeDef Class="ScavengingExpansion.Defs.ScavengingRecipeDef">
        <defName>SE_ScrapMechanoidVariable</defName>
        <label>salvage mechanoid</label>
        <description>Attempt to salvage scraps from a dead mechanoid.</description>
        <jobString>Salvaging mechanoid.</jobString>
        <workAmount>500</workAmount>
        <soundWorking>Recipe_ButcherCorpseMechanoid</soundWorking>
        <workSpeedStat>ButcheryMechanoidSpeed</workSpeedStat>
        <workSkill>Crafting</workSkill>
        <efficiencyStat>ButcheryMechanoidEfficiency</efficiencyStat>
        <effectWorking>ButcherMechanoid</effectWorking>
        <ingredients>
          <li>
            <filter>
              <categories>
                <li>Corpses</li>
              </categories>
            </filter>
            <count>1</count>
          </li>
        </ingredients>
        <scavengingTypes>
          <li>Basic</li>
        </scavengingTypes>
        <recipeUsers>
          <li>SE_AdvancedSalvagingTable</li>
        </recipeUsers>
      </RecipeDef>

      <RecipeDef Class="ScavengingExpansion.Defs.ScavengingRecipeDef">
        <defName>SE_ScrapMechanoidAdvanced</defName>
        <label>carefully salvage mechanoid</label>
        <description>Attempt to salvage rare materials from a dead mechanoid.</description>
        <jobString>Salvaging mechanoid.</jobString>
        <workAmount>500</workAmount>
        <soundWorking>Recipe_ButcherCorpseMechanoid</soundWorking>
        <workSpeedStat>ButcheryMechanoidSpeed</workSpeedStat>
        <workSkill>Crafting</workSkill>
        <efficiencyStat>ButcheryMechanoidEfficiency</efficiencyStat>
        <effectWorking>ButcherMechanoid</effectWorking>
        <ingredients>
          <li>
            <filter>
              <categories>
                <li>Corpses</li>
              </categories>
            </filter>
            <count>1</count>
          </li>
        </ingredients>
        <scavengingTypes>
          <li>Basic</li>
          <li>Advanced</li>
        </scavengingTypes>
        <recipeUsers>
          <li>SE_AdvancedSalvagingTable</li>
        </recipeUsers>
      </RecipeDef>

      <RecipeDef Class="ScavengingExpansion.Defs.ScavengingRecipeDef">
        <defName>SE_SmeltMechanoid</defName>
        <label>smelt mechanoid into alloy</label>
        <description>Smelt a fallen mechanoid to extract some metallic alloy.</description>
        <jobString>Smelting mechanoid.</jobString>
        <workAmount>500</workAmount>
        <soundWorking>Recipe_ButcherCorpseMechanoid</soundWorking>
        <workSpeedStat>ButcheryMechanoidSpeed</workSpeedStat>
        <workSkill>Crafting</workSkill>
        <efficiencyStat>ButcheryMechanoidEfficiency</efficiencyStat>
        <effectWorking>ButcherMechanoid</effectWorking>
        <ingredients>
          <li>
            <filter>
              <categories>
                <li>Corpses</li>
              </categories>
            </filter>
            <count>1</count>
          </li>
        </ingredients>
        <scavengingTypes>
          <li>Alloy</li>
        </scavengingTypes>
        <recipeUsers>
          <li>SE_AlloyForge</li>
        </recipeUsers>
      </RecipeDef>
</Defs>