<Defs>
    <ThingDef ParentName="ArmorSmithableBase">
        <defName>SE_MechanoidAlloyHeavyArmor</defName>
        <label>heavy armor</label>
        <description>Placeholder</description>
        <graphicData>
            <texPath>Things/Pawn/Humanlike/Apparel/MechanoidAlloyArmor/SE_MechanoidAlloyHeavyArmor</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <recipeMaker>
            <researchPrerequisite>AlloyArmor</researchPrerequisite>
			<unfinishedThingDef>UnfinishedMetallicTechArmor</unfinishedThingDef>
			<skillRequirements>
				<Crafting>11</Crafting>
			</skillRequirements>
            <recipeUsers>
                <li>ElectricSmithy</li>
                <li>FueledSmithy</li>
                <li>SE_AdvancedSalvagingTable</li>
                <li>SE_AdvancedSalvagingTable</li>
            </recipeUsers>
		</recipeMaker>
        <costList>
            <Plasteel>60</Plasteel>
            <SE_MechanoidAlloy>35</SE_MechanoidAlloy>
            <Uranium>15</Uranium>
            <ComponentIndustrial>5</ComponentIndustrial>
        </costList>
        <statBases>
            <WorkToMake>38000</WorkToMake>
            <MaxHitPoints>1000</MaxHitPoints>
            <Mass>20</Mass>
            <Flammability>0</Flammability>
            <MarketValue>2100</MarketValue>
            <ArmorRating_Sharp>0.9</ArmorRating_Sharp>
            <ArmorRating_Blunt>0.45</ArmorRating_Blunt>
            <ArmorRating_Heat>0.65</ArmorRating_Heat>
            <Insulation_Cold>4</Insulation_Cold>
            <Insulation_Heat>0</Insulation_Heat>
            <EquipDelay>20</EquipDelay>
        </statBases>
        <thingSetMakerTags>
            <li>RewardStandardLowFreq</li>
            <li>RewardStandardQualitySuper</li>
        </thingSetMakerTags>
        <equippedStatOffsets>
            <MoveSpeed>-0.5</MoveSpeed>
            <MeleeDodgeChance>-5</MeleeDodgeChance>
        </equippedStatOffsets>
        <thingCategories>
            <li>ApparelArmor</li>
        </thingCategories>
        <apparelTag>
            <li>ScavHeavyApparel</li>
        </apparelTag>
        <apparel>
            <bodyPartGroups>
                <li>Torso</li>
                <li>Neck</li>
                <li>Shoulders</li>
                <li>Arms</li>
                <li>Legs</li>
            </bodyPartGroups>
            <wornGraphicPath>Things/Pawn/Humanlike/Apparel/MechanoidAlloyArmor/SE_MechanoidAlloyHeavyArmor</wornGraphicPath>
            <layers>
                <li>Middle</li>
                <li>Shell</li>
            </layers>
            <tags Inherit="False">
                <li>Apparel_Scavenger</li>
                <li>Apparel_ScavengerMilitary</li>
            </tags>
            <defaultOutfitTags>
                <li>Soldier</li>
            </defaultOutfitTags>
        </apparel>
    </ThingDef>

