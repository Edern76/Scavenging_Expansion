            </layers>
            <hatRenderedAboveBody>false</hatRenderedAboveBody>
            <canBeDesiredForIdeo MayRequire="Ludeon.RimWorld.Ideology">true</canBeDesiredForIdeo>
            <tags>
                <li>ScavRareApparel</li>
            </tags>
        </apparel>
    </ThingDef>

