        </costList>
        <recipeMaker>
            <recipeUsers>
              <li>ElectricSmithy</li>
              <li>FueledSmithy</li>
              <li>SE_SalvagingTable</li>
              <li>SE_AdvancedSalvagingTable</li>
            </recipeUsers>
        </recipeMaker>
        <statBases>
            <WorkToMake>1600</WorkToMake>
            <MaxHitPoints>100</MaxHitPoints>
            <Mass>2</Mass>
            <ArmorRating_Sharp>0.4</ArmorRating_Sharp>
            <ArmorRating_Blunt>0.4</ArmorRating_Blunt>
            <ArmorRating_Heat>0.4</ArmorRating_Heat>
            <Insulation_Cold>1</Insulation_Cold>
            <Insulation_Heat>1</Insulation_Heat>
            <EquipDelay>1.5</EquipDelay>
        </statBases>
        <equippedStatOffsets>
            <SocialImpact>0.1</SocialImpact>
            <TradePriceImprovement>0.1</TradePriceImprovement>
        </equippedStatOffsets>
        <apparel>
            <bodyPartGroups>
                <li>UpperHead</li>
            </bodyPartGroups>
            <wornGraphicPath>Things/Pawn/Humanlike/Apparel/ScrapCrown/SE_ScrapCrown</wornGraphicPath>
