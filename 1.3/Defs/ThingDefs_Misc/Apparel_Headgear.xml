<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <ThingDef ParentName="ArmorHelmetMakeableBase">
        <defName>SE_MechanoidAlloyHelmet</defName>
        <label>mechanoid alloy helmet</label>
        <description>Hard and bulky, the mechaoid alloy helmet is a favorite of many for its above average protection.</description>
        <graphicData>
            <texPath>Things/Pawn/Humanlike/Apparel/MechanoidAlloyHelmet/SE_MechanoidAlloyHelmet</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <costList>
            <Plasteel>35</Plasteel>
            <SE_MechanoidAlloy>35</SE_MechanoidAlloy>
            <Uranium>15</Uranium>
            <ComponentIndustrial>5</ComponentIndustrial>
        </costList>
        <recipeMaker>
            <skillRequirements>
				<Crafting>11</Crafting>
			</skillRequirements>
            <recipeUsers>
              <li>ElectricSmithy</li>
              <li>FueledSmithy</li>
              <li>SE_SalvagingTable</li>
              <li>SE_AdvancedSalvagingTable</li>
            </recipeUsers>
        </recipeMaker>
        <statBases>
            <WorkToMake>14000</WorkToMake>
            <MaxHitPoints>250</MaxHitPoints>
            <MarketValue>450</MarketValue>
            <Mass>5</Mass>
            <ArmorRating_Sharp>0.9</ArmorRating_Sharp>
            <ArmorRating_Blunt>0.4</ArmorRating_Blunt>
            <ArmorRating_Heat>0.60</ArmorRating_Heat>
            <Insulation_Cold>0</Insulation_Cold>
            <Insulation_Heat>3</Insulation_Heat>
            <EquipDelay>6</EquipDelay>
        </statBases>
        <thingSetMakerTags>
            <li>RewardStandardLowFreq</li>
            <li>RewardStandardQualitySuper</li>
        </thingSetMakerTags>
        <equippedStatOffsets>
            <MoveSpeed>-0.2</MoveSpeed>
            <ShootingAccuracyPawn>-0.5</ShootingAccuracyPawn>
        </equippedStatOffsets>
        <apparel>
            <bodyPartGroups>
                <li>FullHead</li>
            </bodyPartGroups>
            <wornGraphicPath>Things/Pawn/Humanlike/Apparel/MechanoidAlloyHelmet/SE_MechanoidAlloyHelmet</wornGraphicPath>
            <layers>
                <li>Overhead</li>
            </layers>
            <tags Inherit="False">
                <li>Apparel_Scavenger</li>
                <li>Apparel_ScavengerMilitary</li>
            </tags>
            <defaultOutfitTags>
                <li>Soldier</li>
            </defaultOutfitTags>
            <hatRenderedAboveBody>false</hatRenderedAboveBody>
        </apparel>
    </ThingDef>

    <ThingDef ParentName="ArmorHelmetMakeableBase">
        <defName>SE_SalvagedLightHelmet</defName>
        <label>salvaged light helmet</label>
        <description>Metals twisted together to form a helmet, even if mocked the sturdy design offer a decent protection.</description>
        <graphicData>
            <texPath>Things/Pawn/Humanlike/Apparel/ScavLightHelmet/SE_ScavLightHelm</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <stuffCategories>
            <li>Metallic</li>
        </stuffCategories>
        <costStuffCount>45</costStuffCount>
        <recipeMaker>
            <recipeUsers>
              <li>ElectricSmithy</li>
              <li>FueledSmithy</li>
              <li>SE_SalvagingTable</li>
              <li>SE_AdvancedSalvagingTable</li>
            </recipeUsers>
            <memePrerequisitesAny>
                <li>Supremacist</li> <!-- TODO : Replace with scavenger meme-->
            </memePrerequisitesAny>
        </recipeMaker>
        <statBases>
            <WorkToMake>3200</WorkToMake>
            <MaxHitPoints>100</MaxHitPoints>
            <Mass>2</Mass>
            <StuffEffectMultiplierArmor>0.6</StuffEffectMultiplierArmor>
            <StuffEffectMultiplierInsulation_Cold>0.2</StuffEffectMultiplierInsulation_Cold>
            <StuffEffectMultiplierInsulation_Heat>0</StuffEffectMultiplierInsulation_Heat>
            <EquipDelay>1.5</EquipDelay>
        </statBases>
        <thingSetMakerTags>
            <li>RewardStandardLowFreq</li>
        </thingSetMakerTags>
        <apparel>
            <bodyPartGroups>
                <li>UpperHead</li>
            </bodyPartGroups>
            <wornGraphicPath>Things/Pawn/Humanlike/Apparel/ScavLightHelmet/SE_ScavLightHelm</wornGraphicPath>
            <layers>
                <li>Overhead</li>
            </layers>
            <tags Inherit="False">
                <li>Apparel_Scavenger</li>
                <li>Apparel_ScavengerMilitary</li>
            </tags>
            <defaultOutfitTags>
                <li>Soldier</li>
            </defaultOutfitTags>
            <hatRenderedAboveBody>false</hatRenderedAboveBody>
        </apparel>
    </ThingDef>

    <ThingDef ParentName="ArmorHelmetMakeableBase">
