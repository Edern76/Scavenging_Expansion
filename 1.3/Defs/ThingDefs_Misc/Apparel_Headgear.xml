    <ThingDef ParentName="ArmorHelmetMakeableBase">
        <defName>SE_DominionCrownHelmet</defName>
        <label>dominion crown</label>
        <description>Masterfully handcrafted crown, fit for a king. Psychic energy seems to emanate from within the golden halo.</description>
        <graphicData>
            <texPath>Things/Pawn/Humanlike/Apparel/DominionCrown/SE_DominionCrown</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <costList>
            <SE_DominionCrown_Resource>1</SE_DominionCrown_Resource>
            <SE_MechanoidAlloy>10</SE_MechanoidAlloy>
            <Plasteel>40</Plasteel>
        </costList>
        <recipeMaker>
            <researchPrerequisite>DominionMight</researchPrerequisite>
			<skillRequirements>
				<Crafting>10</Crafting>
			</skillRequirements>
            <recipeUsers Inherit="False">
                <li>SE_AdvancedSalvagingTable</li>
            </recipeUsers>
		</recipeMaker>
        <statBases>
            <WorkToMake>14000</WorkToMake>
            <MaxHitPoints>180</MaxHitPoints>
            <Mass>5</Mass>
            <ArmorRating_Sharp>0.90</ArmorRating_Sharp>
            <ArmorRating_Blunt>0.30</ArmorRating_Blunt>
            <ArmorRating_Heat>0.60</ArmorRating_Heat>
            <Insulation_Cold>2.5</Insulation_Cold>
            <Insulation_Heat>4</Insulation_Heat>
            <EquipDelay>6</EquipDelay>
        </statBases>
        <equippedStatOffsets>
            <PsychicSensitivity MayRequire="Ludeon.RimWorld.Royalty">0.25</PsychicSensitivity>
            <PsychicEntropyMax MayRequire="Ludeon.RimWorld.Royalty">4</PsychicEntropyMax>
            <PsychicEntropyRecoveryRate MayRequire="Ludeon.RimWorld.Royalty">0.04</PsychicEntropyRecoveryRate>
            <CertaintyLossFactor MayRequire="Ludeon.RimWorld.Ideology">-0.20</CertaintyLossFactor>
