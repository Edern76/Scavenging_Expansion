<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <ThingDef ParentName="BaseMeleeWeapon_Sharp_Quality">
        <thingClass>ThingWithComps</thingClass>
        <defName>SE_MeleeWeapon_Scyther</defName>
        <label>the scyther</label>
        <description>Trusty friend or bane of the prisoner, the scyther named after the mechanoid is often used as an executionner tool but can be seen amongst the mechanoid hunters.</description>
        <graphicData>
          <texPath>Things/Item/Equipment/WeaponMelee/TheScyther</texPath>
          <graphicClass>Graphic_Single</graphicClass>
          <drawSize>1.3</drawSize>
        </graphicData>
        <techLevel>Industrial</techLevel>
        <weaponTags>
          <li>ScavengerMeleeAdvanced</li>
          <li>Axe</li>
        </weaponTags>
        <weaponClasses>
            <li>Scavengers</li>
        </weaponClasses>
        <costList>
            <SE_MechanoidAlloy>40</SE_MechanoidAlloy>
            <SE_MechanoidScrap>10</SE_MechanoidScrap>
            <Plasteel>50</Plasteel>
        </costList>
        <thingSetMakerTags><li>RewardStandardQualitySuper</li></thingSetMakerTags>
        <statBases>
          <WorkToMake>18000</WorkToMake>
          <Mass>2</Mass>
        </statBases>
        <relicChance>2</relicChance>
        <equippedAngleOffset>-65</equippedAngleOffset>
        <tools>
          <li>
            <label>handle</label>
            <capacities>
              <li>Blunt</li>
            </capacities>
            <power>10</power>
            <cooldownTime>2</cooldownTime>
          </li>
          <li>
            <label>edge</label>
            <capacities>
              <li>Cut</li>
            </capacities>
            <power>26</power>
            <cooldownTime>2.5</cooldownTime>
            <armorPenetration>0.7</armorPenetration>
          </li>
        </tools>
        <recipeMaker>
            <recipeUsers>
                <li>FueledSmithy</li>
                <li>ElectricSmithy</li>
                <li>SE_SalvagingTable</li>
                <li>SE_AdvancedSalvagingTable</li>
            </recipeUsers>
            <workSkill>Crafting</workSkill>
            <effectWorking>Smith</effectWorking>
            <soundWorking>Recipe_Smith</soundWorking>
            <unfinishedThingDef>UnfinishedWeapon</unfinishedThingDef>
        </recipeMaker>
        <comps>
            <li Class="CompOversizedWeapon.CompProperties_OversizedWeapon"></li>
        </comps>
      </ThingDef>

