<Defs>
    <ThingDef ParentName="BaseMeleeWeapon_Sharp_Quality">
        <thingClass>ThingWithComps</thingClass>
        <defName>SE_MeleeWeapon_ScrapSword</defName>
        <label>scrap sword</label>
        <description>The usual cut-throat used by many within the outlands.</description>
        <graphicData>
          <texPath>Things/Item/Equipment/WeaponMelee/ScrapSword</texPath>
          <graphicClass>Graphic_Single</graphicClass>
          <drawSize>1.5</drawSize>
        </graphicData>
        <techLevel>Medieval</techLevel>
        <weaponTags>
          <li>ScavengerMeleeAdvanced</li>
          <li>LongSword</li>
        </weaponTags>
        <weaponClasses>
            <li>Scavengers</li>
        </weaponClasses>
        <costList>
            <SE_MechanoidScrap>20</SE_MechanoidScrap>
            <Steel>50</Steel>
        </costList>
        <thingSetMakerTags><li>RewardStandardQualitySuper</li></thingSetMakerTags>
        <statBases>
          <MarketValue>280</MarketValue>
          <WorkToMake>19000</WorkToMake>
          <Mass>2.25</Mass>
          <MeleeWeapon_AverageArmorPenetration>0.35</MeleeWeapon_AverageArmorPenetration>
        </statBases>
        <relicChance>2</relicChance>
        <equippedAngleOffset>-65</equippedAngleOffset>
        <tools>
          <li>
            <label>handle</label>
            <capacities>
              <li>Blunt</li>
            </capacities>
            <power>10</power>
            <cooldownTime>2</cooldownTime>
          </li>
          <li>
            <label>point</label>
            <capacities>
              <li>Stab</li>
            </capacities>
            <power>25</power>
            <cooldownTime>2.6</cooldownTime>
          </li>
          <li>
            <label>blade</label>
            <capacities>
              <li>Cut</li>
            </capacities>
            <power>25</power>
            <cooldownTime>2.6</cooldownTime>
          </li>
        </tools>
        <recipeMaker>
          <skillRequirements>
            <Crafting>4</Crafting>
          </skillRequirements>
          <recipeUsers>
            <li>FueledSmithy</li>
            <li>ElectricSmithy</li>
          </recipeUsers>
          <workSkill>Crafting</workSkill>
          <effectWorking>Smith</effectWorking>
          <soundWorking>Recipe_Smith</soundWorking>
        </recipeMaker>
        <comps>
            <li Class="CompOversizedWeapon.CompProperties_OversizedWeapon">
              <eastOffset>(0.31,0,0)</eastOffset>
              <westOffset>(-0.31,0,0)</westOffset>
            </li>
        </comps>
      </ThingDef>

