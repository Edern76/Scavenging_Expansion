<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <WorkGiverDef>
        <defName>SE_DoBillsSalvage</defName>
        <label>salvage things</label>
        <giverClass>WorkGiver_DoBill</giverClass>
        <workType>Crafting</workType>
        <priorityInType>80</priorityInType>
        <fixedBillGiverDefs>
            <li>SE_AdvancedSalvagingTable</li>
        </fixedBillGiverDefs>
        <verb>salvage</verb>
        <gerund>salvaging at</gerund>
        <requiredCapacities>
            <li>Manipulation</li>
        </requiredCapacities>
        <prioritizeSustains>true</prioritizeSustains>
    </WorkGiverDef>

    <WorkGiverDef>
        <defName>SE_Excavate</defName>
        <label>operate an excavator to unearth forgotten riches</label>
        <giverClass>ScavengingExpansion.Jobs.WorkGivers.WorkGiver_Excavate</giverClass>
        <workType>Excavating</workType>
        <priorityInType>100</priorityInType>
        <verb>excavate at</verb>
        <gerund>excavating at</gerund>
        <requiredCapacities>
            <li>Manipulation</li>
        </requiredCapacities>
    </WorkGiverDef>
</Defs>